import java.util.Scanner;
public class ex4 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.println("Digite um numero=");
        int a = input.nextInt();
        boolean istrue = true;

        for(int i = 2;i < a / 2;i++){
            if(a % i == 0){
                istrue = false;

            }
        }
        if(istrue){
            System.out.println("é primo");

        }else {
            System.out.println("náo é primo");
        }
    }
}
